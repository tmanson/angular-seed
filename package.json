{
  "name": "angular-8-seed",
  "version": "0.0.1",
  "author": "Sergi Mart√≠nez",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:app-catalog": "ng serve app-catalog",
    "build": "npm run build:libraries && npm run build:clients && npm run build:main",
    "build:libraries": "ng build --project shared-lib",
    "build:clients": "npm run build:app-home && npm run build:app-administration && npm run build:app-catalog",
    "deploy:frontends": "cp -R src/assets/micro-frontends dist/main/assets/",
    "build:app-home": "ng build --single-bundle --project app-home --output-hashing none --vendor-chunk false --output-path src/assets/micro-frontends/app-home",
    "build:app-administration": "ng build --single-bundle --project app-administration --output-hashing none --vendor-chunk false --output-path src/assets/micro-frontends/app-administration",
    "build:app-catalog": "ng build --single-bundle --project app-catalog --output-hashing none --vendor-chunk false --output-path src/assets/micro-frontends/app-catalog",
    "build:main": "ng build --project main",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "ng build --prod --aot=false --build-optimization=false"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.901.1",
    "@angular/animations": "^9.1.1",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "^9.1.1",
    "@angular/compiler": "^9.1.1",
    "@angular/core": "^11.0.5",
    "@angular/elements": "^9.1.1",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.1.1",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "^9.1.1",
    "@angular/platform-browser-dynamic": "^9.1.1",
    "@angular/pwa": "^0.901.1",
    "@angular/router": "^9.1.1",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "chartist": "^0.11.4",
    "core-js": "^3.6.5",
    "jwt-decode": "^2.2.0",
    "ng-chartist": "^4.1.0",
    "ngx-build-plus": "^9.0.6",
    "ngx-webstorage": "^5.0.0",
    "rxjs": "^6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.1",
    "@angular-devkit/build-ng-packagr": "^0.901.1",
    "@angular/cli": "^9.1.1",
    "@angular/compiler-cli": "^9.1.1",
    "@angular/language-service": "^9.1.1",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~13.11.1",
    "@types/rx": "^4.1.1",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ng-packagr": "^9.1.1",
    "protractor": "^5.4.3",
    "ts-node": "~8.8.2",
    "tslint": "^6.1.1",
    "typescript": "~3.8.3"
  }
}
